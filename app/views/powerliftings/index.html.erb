<h1>Listing powerliftings</h1>

<table>
  <thead>
    <tr>
      <th>Move</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @powerliftings.each do |powerlifting| %>
      <tr>
        <td><%= powerlifting.move %></td>
        <td><%= powerlifting.result %></td>
        <td><%= link_to 'Show', powerlifting %></td>
        <td><%= link_to 'Edit', edit_powerlifting_path(powerlifting) %></td>
        <td><%= link_to 'Destroy', powerlifting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Powerlifting', new_powerlifting_path %>
